{
  "name": "rudracomputech/laravel-delhivery-api",
  "description": "Laravel package for Delhivery B2C API integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rudra Computech",
      "email": "info@rudracomputech.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Rudracomputech\\Delhivery\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rudracomputech\\Delhivery\\DelhiveryServiceProvider"
      ],
      "aliases": {
        "Delhivery": "Rudracomputech\\Delhivery\\Facades\\Delhivery"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}